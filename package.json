{
	"name": "inset",
	"description": "Inset - A Webview API Sample",
	"version": "0.0.1",
	"publisher": "lollobaldo",
	"private": true,
	"license": "MIT",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:myExtension.sayHello"
	],
	"main": "./out/extension.js",
	"enableProposedApi": true,
	"contributes": {
		"commands": [
			{
				"command": "myExtension.sayHello",
				"title": "Demo session",
				"category": "Inset:"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.47.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.4.3"
	}
}
